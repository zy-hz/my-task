.custom_tab {
    bottom: 0;
    position: fixed;
    height: 120rpx;
    border-top: 1px solid #DCDCDC;
    width: 100%;
    display: flex;
    flex-direction: row;
    justify-content: space-around;
    align-items: center;
    font-size: 28rpx;
    background-color: #F6F6F6;
}
.custom_tab_item {
    display: flex;
    flex-direction: column;
    align-items: center;
}
.tab_item_img {
    width: 56rpx;
    height: 52rpx;
}
.public_tab {
    position: fixed;
    top: 87%;
    width: 114rpx;
    height: 102rpx;
}
.public_title {
    position: fixed;
    top: 96.8%;
}
.tab_item_title {
    margin-top: 12rpx;
}
.select_title {
    color: red;
}
.task-list__page {
  background-color: #f8f8f8;
  font-size: 16rpx;
  font-family: -apple-system-font, Helvetica Neue, Helvetica, sans-serif;
}

.task-list__hd {
  height: 1px;
}

.task-list__ft {
  height: 110rpx;
  text-align: center;
}

.task-item__info {
  display: inline;
}

.course__hd {
  background-color: #f8f8f8;
}

.custom_tab {
  bottom: 0;
  position: fixed;
  height: 110rpx;
  border-top: 1px solid #dcdcdc;
  width: 100%;
  display: flex;
  flex-direction: row;
  justify-content: space-around;
  align-items: center;
  font-size: 28rpx;
  background-color: #fffffb;
  z-index: 999;
}

.custom_tab .weui-cell {
  width: 100%;
}

.tab_item_img {
  width: 64rpx;
  height: 64rpx;
  padding-left: 40rpx;
}
